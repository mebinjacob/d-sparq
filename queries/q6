SELECT ?yr ?name ?document WHERE { ?class <http://www.w3.org/2000/01/rdf-schema#subClassOf> <http://xmlns.com/foaf/0.1/Document> . ?document <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ?class . ?document <http://purl.org/dc/terms/issued> ?yr . ?document <http://purl.org/dc/elements/1.1/creator> ?author . ?author <http://xmlns.com/foaf/0.1/name> ?name . }