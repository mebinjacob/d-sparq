SELECT ?subject ?predicate ?object WHERE { ?class <http://www.w3.org/2000/01/rdf-schema#subClassOf> <http://xmlns.com/foaf/0.1/Document> . ?doc <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ?class . ?doc <http://purl.org/dc/elements/1.1/title> ?title . ?bag2 ?member2 ?doc . ?doc2 <http://purl.org/dc/terms/references> ?bag2 . ?subject ?predicate ?object . }